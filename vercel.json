{
    "version": 2,
    "builds": [
        {
            "src": "api/index.js",
            "use": "@vercel/node",
            "config": {
                "includeFiles": [
                    "../views/**",
                    "../public/**"
                ]
            }
        }
    ],
    "routes": [
        {
            "src": "/(.*)",
            "dest": "/server.js"
        }
    ]
}